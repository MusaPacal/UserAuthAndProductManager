@using EntityLayer.Concrete
@model List<Job>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_UILayout.cshtml";
}

<h1>Meslekler</h1>

<table class="table table-bordered">
    <tr>
        <th>Meslek No</th>
        <th>Meslek Adı</th>
        <th>Güncelle</th>
        <th>Sil</th>
        <th>Detaylar</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.JobID</td>
            <td>@item.JobName</td>
            <td><a href="/Job/UpdateJob/@item.JobID" class="btn btn-warning">Güncelle</a></td>
            <td><a href="/Job/DeleteJob/@item.JobID" class="btn btn-danger">Sil</a></td>
            <td><a href="/Job/DetailsJob/@item.JobID" class="btn btn-info">Detaylar</a></td>
        </tr>

    }
</table>